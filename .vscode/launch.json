{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch cppPlay",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cppPlay/main.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/cppPlay",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
